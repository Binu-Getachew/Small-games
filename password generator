#passward generator
import random
import string

n = 3  
nums=''.join(random.choice(string.digits) for _ in range(n))
letters = ''.join(random.choice(string.ascii_uppercase) for _ in range(n))
s_letters = ''.join(random.choice(string.ascii_lowercase) for _ in range(n))
special_chars = ''.join(random.choice(string.punctuation) for _ in range(n))

password=(nums+special_chars+letters+s_letters)
password_list=list(password)
random.shuffle(password_list)
final=''.join(password_list)
print(final)
